class {{name}}
{
  {{#color}}
  color[] = { {{color.r}}, {{color.g}}, {{color.b}}, {{color.a}} };
  {{/color}}
  class Line {
    type = "line";
    lineType = "{{lineType}}";
    width = {{width}};
    points[] = {
      {{#points}}
        { {{#bone}}"{{bone}}",{{/bone}} { {{x}}, {{y}} }, 1}{{#comma}},{{/comma}}
      {{/points}}
    };
  };
};//End of {{name}}